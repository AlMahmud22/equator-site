{
  "name": "equator-frontend",
  "version": "1.0.0",
  "description": "Official equator brand website - Showcase and distribute desktop apps",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node server.js",
