{
  "name": "equators-backend",
  "version": "1.0.0",
  "description": "Backend server for Equators platform",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
